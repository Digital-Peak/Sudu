{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/essential",
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "semi": ["error", "always"],
        "indent": [2, "tab"],
        "no-tabs": 0,
        "object-curly-spacing": ["error", "never"],
        "space-before-function-paren": ["error", "never"],
        "no-return-assign": 0
    }
}
